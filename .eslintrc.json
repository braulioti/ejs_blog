{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": "off"
    },
    "no-trailing-spaces": "ignore",
    "indent": ["error", 4]
}
